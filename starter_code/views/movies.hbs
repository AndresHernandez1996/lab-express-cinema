<div class="textMovies">
  <h1>Ironhack Cinema!</h1>
  <p>Click on the movie to see the show times.</p>
</div>
<section class="movies">
  {{#each movies}}
  <div class="moviesView">
    <img src="{{image}}" alt="" srcset="" />
    <p>{{title}}</p>
    <a href="/movie/{{_id}}">See more</a>
  </div>
  {{/each}}
</section>